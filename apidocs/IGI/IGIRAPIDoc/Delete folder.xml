<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="RAPI.xsl"?>
<application>
  <doc>Delete folder</doc>
  <resources base="">
    <resource path="https://{appliance_hostname}:9443/custfile_mgmt/delete_folder">
      <param type="path" name="appliance_hostname">
        <doc>Host name of the appliance.</doc>
      </param>
      <method name="DELETE" id="Delete folder">
        <doc>     To delete a folder in your appliance with the RESTful web service, issue an HTTP DELETE request on the custom file resource URI. </doc>
        <request>
          <doc>DELETE https://{appliance_hostname}:9443/custfile_mgmt/delete_folder
 
 DELETE DATA
 
 {
  folderPath: "properties/testfolder"
 }</doc>
          <param name="Content-Type:application/json" style="header">
            <doc> Required for requests to the service.@rp_end_api/</doc>
          </param>
          <param name="Authorization" style="header">
            <doc>Basic Authentication header. </doc>
          </param>
          <param name="Accept:application/json" style="header">
            <doc> Required for requests to the service.</doc>
          </param>
        </request>
        <response status="200">
          <doc>OK</doc>
          <doc>
 200 OK
</doc>
        </response>
      </method>
    </resource>
  </resources>
</application>
