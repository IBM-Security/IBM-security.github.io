<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="RAPI.xsl"?>
<application>
  <doc>Delete a property from the cluster configuration</doc>
  <resources base="">
    <resource path="https://{appliance_hostname}/isam/cluster/property/{id}/v2">
      <param type="path" name="id">
        <doc>The name of the cluster property</doc>
      </param>
      <param type="path" name="appliance_hostname">
        <doc>Host name of the appliance.</doc>
      </param>
      <method name="DELETE" id="DeleteProperty">
        <doc>   This web service will delete an existing property from the current cluster configuration information.<br/>                    <p>Note: An initial configuration create must have been executed before this web service can be run.</p>                    <p>Note: This web service can only be run on the primary master.</p> </doc>
        <request>
          <doc>DELETE https://{appliance_hostname}/isam/cluster/property/secondary_master/v2</doc>
          <param name="Authorization" style="header">
            <doc>Basic Authentication header </doc>
          </param>
          <param name="Accept:application/json" style="header">
            <doc>Required for requests to the service </doc>
          </param>
        </request>
        <response status="204">
          <doc>The request was successful.</doc>
          <doc>
 204 No Content
 Content-Type:application/json
 </doc>
          <param name="Content-Type:application/json" style="header">
            <doc>Response is in JSON formatting </doc>
          </param>
        </response>
        <response status="400">
          <doc>There is a problem with the request. The JSON message describes the problem.</doc>
          <param name="Content-Type:application/json" style="header">
            <doc>Response is in JSON formatting </doc>
          </param>
          <representation name="httpErrorJsonResponse" mediatype="application/json">
            <param style="plain" name="message" type="String" optional="No">
              <doc>The error message. </doc>
            </param>
          </representation>
        </response>
        <response status="404">
          <doc>File not Found, The requested service does not exist. The JSON message indicates which resource.</doc>
          <param name="Content-Type:application/json" style="header">
            <doc>Response is in JSON formatting </doc>
          </param>
          <representation name="httpErrorJsonResponse" mediatype="application/json">
            <param style="plain" name="message" type="String" optional="No">
              <doc>The error message. </doc>
            </param>
          </representation>
        </response>
        <response status="500">
          <doc>An internal error occurred. The JSON message indicates the problem.</doc>
          <param name="Content-Type:application/json" style="header">
            <doc>Response is in JSON formatting </doc>
          </param>
          <representation name="httpErrorJsonResponse" mediatype="application/json">
            <param style="plain" name="message" type="String" optional="No">
              <doc>The error message. </doc>
            </param>
          </representation>
        </response>
      </method>
    </resource>
  </resources>
</application>
