<?xml version = "1.0" encoding ="UTF-8" ?>
<?xml-stylesheet type="text/xsl" href="RAPI.xsl"?>
<application><doc>Get all email objects</doc>
<resources base="">
<resource path="https://{appliance_hostname}/rsp_email_objs">
<method name="GET" id="Get all email response objects."><doc>      Get all email objects. There could be no members </doc>
<request>
<doc>GET https://{appliance_hostname}/rsp_email_objs</doc><param name="Authorization" style="header">
<doc>Basic Authentication header </doc></param>
<param name="Accept:application/json" style="header">
<doc> Required for requests to the service</doc></param>
<param name="Range:items={start}-{end}" style="header">
<doc> Range to limit the number of items retrieved</doc></param>
</request>
<response status="400">
<doc>Bad request</doc>
<param name="Content-Type:application/json" style="header">
<doc> Response is in JSON formatting</doc></param>
<param name="Content-Range:{start}-{end}/{total}" style="header">
<doc> Item range retrieved</doc></param>
<representation name="httpErrorJsonResponse" mediatype="application/json">
<param style="plain" name="message" type="String" optional="No" >
<doc>The error message. </doc>
</param>
</representation></response><response status="404">
<doc>Specified object can not be found</doc>
<param name="Content-Type:application/json" style="header">
<doc> Response is in JSON formatting</doc></param>
<param name="Content-Range:{start}-{end}/{total}" style="header">
<doc> Item range retrieved</doc></param>
<representation name="httpErrorJsonResponse" mediatype="application/json">
<param style="plain" name="message" type="String" optional="No" >
<doc>The error message. </doc>
</param>
</representation></response><response status="500">
<doc>An internal error occurred. The JSON message indicates the problem.</doc>
<param name="Content-Type:application/json" style="header">
<doc> Response is in JSON formatting</doc></param>
<param name="Content-Range:{start}-{end}/{total}" style="header">
<doc> Item range retrieved</doc></param>
<representation name="httpErrorJsonResponse" mediatype="application/json">
<param style="plain" name="message" type="String" optional="No" >
<doc>The error message. </doc>
</param>
</representation></response><response status="200">
<doc>Request Successful</doc>
<doc>
  200 OK
  Content-Type:application/json
  {
    "emailObjects": [
      {
         {
       "smtpServer": "1.1.1.1",
       "smtpPort": 25,
       "from": "a@a.com",
       "to": "b@b.com",
       "name": "emailObject",
       "uuid": "893c8086-91cb-4f88-953f-0128252ff2e4",
       "objType": "email",
       "comment": "Log entries are sent to SMTP server configured."

      }
    ]
  }
 </doc><param name="Content-Type:application/json" style="header">
<doc> Response is in JSON formatting</doc></param>
<param name="Content-Range:{start}-{end}/{total}" style="header">
<doc> Item range retrieved</doc></param>
<representation name="EmailObjects_Get" mediatype="application/json">
<param style="plain" name="emailObjects" type="EmailObject_Get" optional="No"  repeating="true">
<doc>Collection of Email Objects. </doc>
</param>
</representation><representation name="EmailObject_Get" mediatype="application/json">
<param style="plain" name="smtpServer" type="String" optional="No" >
<doc>SMTP Server configuration</doc>
</param>
<param style="plain" name="smtpPort" type="Integer" optional="No" >
<doc>SMTP Port configuration</doc>
</param>
<param style="plain" name="from" type="String" optional="No" >
<doc>SMTP envelope-sender configuration</doc>
</param>
<param style="plain" name="to" type="String" optional="No" >
<doc>Percentage (expressed as a whole number 0-100) of the total event storage limit to allocate for IPS Events.</doc>
</param>
<param style="plain" name="name" type="String" optional="No" >
<doc>Name of the object</doc>
</param>
<param style="plain" name="uuid" type="String" optional="No" >
<doc>Unique id of the object.</doc>
</param>
<param style="plain" name="objType" type="String" optional="No" >
<doc>Type of the object (always "email").</doc>
</param>
<param style="plain" name="comment" type="String" optional="No" >
<doc>Comment which describes the object (always the same, but locale dependent). </doc>
</param>
</representation></response></method>
</resource>
</resources>
</application>
